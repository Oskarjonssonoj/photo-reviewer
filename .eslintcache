) => {\n        if(!newAlbum) {\n            return\n        }\n        db.collection('albums').doc(newAlbum).set({\n            name: newAlbum\n        })\n        setNewAlbum(\"\")\n    }\n\n\n    useEffect(() => {\n        const unmount = db.collection('albums').onSnapshot((snapshot) => {\n            const snapAlbum = []\n            snapshot.forEach(doc => {\n                snapAlbum.push({...doc.data(), id: doc.id});\n            })\n            setAlbums(snapAlbum)\n        })\n        return unmount;\n    }, [])\n\n    return (\n        <div className=\"startpage\">\n            <nav>\n                <h2>Welcome</h2>\n                <div className=\"topBtns\">\n                    <div className=\"addAlbumSection\">\n                        <input placeholder=\"Album name...\" type=\"text\" value={newAlbum} onChange={albumNameChange}/>\n                        <button \n                            className=\"addAlbum\" \n                            onClick={createAlbum}>Add Album</button>\n                    </div>\n                    <button onClick={handleLogout}>Logout</button>\n                </div>\n            </nav>\n            \n            <div className=\"albumsSection\">\n                <div className=\"albums\">\n                    <Switch>\n                        <Route exact path=\"/\">\n                            <Albums albums={albums}/>\n                        </Route>\n                        <Route path=\"/:album\">\n                            <Album />\n                        </Route>\n                                 \n                    </Switch>\n                </div>\n            </div>\n\n        </div>\n    )\n}\n\nexport default Startpage\n","/Applications/MAMP/htdocs/FED19/photo-reviewer/src/components/Albums/Albums.js",["49","50","51","52","53","54","55","56"],"import React, { useState, useEffect } from 'react'\nimport './styles/albums.scss'\nimport AlbumCoverOne from './images/one.png'\nimport {db, storage} from