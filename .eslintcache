",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/ForgotPassword/ForgotPassword.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/Signup/Signup.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/RestrictedRoute/RestrictedRoute.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/firebase/firebase.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/Albums/Albums.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/Albums/Album.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/Navigation/NavigationBar.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/Photos/NewPhoto.js",["109"],"import React, { useState } from 'react'\nimport firebase from 'firebase'\nimport { db, storage } from '../../firebase/firebase'\nimport { Progress } from \"@chakra-ui/react\"\n\nimport { Button, ButtonGroup } from \"@chakra-ui/react\"\n\n\n\nconst NewPhoto = ({currentAlbum}) => {\n\n    const [file, setFile] = useState(null)\n    const [alertMsg, setAlertMsg] = useState(null)\n    const [uploadProgress, setUploadProgress] = useState(null)\n\n    const onFileChange = (e) => {\n        setFile(e.target.files[0])\n    }\n\n   \n    const onUpload = async e => {\n        e.preventDefault()\n\n        const storageRef = storage.ref()\n        const fileRef = storageRef.child(file.name)\n        const uploadTask = fileRef.put(file)\n        \n        uploadTask.on('state_change', taskSnapshot =>{\n            setUploadProgress((taskSnapshot.bytesTransferred / taskSnapshot.totalBytes) * 100);\n            console.log('uploadProgress')\n        })\n        await uploadTask.then( snapshot =>{\n            console.log('file has been upload', snapshot)\n            setUploadProgress(null)\n            setFile(null)\n            setAlertMsg({\n                type: 'success',\n                msg: \"Image successfully uploaded\"\n            })\n        }).catch( error =>{\n            console.log('file upload a error', error)\n            setAlertMsg({\n                type: 'warning',\n                msg: `Image could not be uploaded due to an error ${error.code}`\n            })\n        })\n        \n        \n        db.collection(\"albums\").doc(currentAlbum).update({\n            images: firebase.firestore.FieldValue.arrayUnion({\n                name: file.name,\n                url: await fileRef.getDownloadURL()\n            })\n        })\n    }\n\n    return (\n        <>\n            <div>\n                <input type=\"file\" onChange={onFileChange} />\n                {\n                    alertMsg && (<div>{alertMsg.msg}</div>)\n                }\n            </div>\n                {\n                    uploadProgress !== null && (<Progress value={uploadProgress} size=\"xs\" colorScheme=\"pink\" />)\n                }\n            <Button \n                onClick={onUpload} \n                colorScheme=\"green\" \n                size=\"lg\" \n                width=\"600px\" \n                > Upload image </Button>\n        </>\n    )\n}\n\nexport default NewPhoto\n","/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/Photos/DropzoneUpload.js",["110","111","112"],"import React, {useState, useEffect, useCallback} from 'react'\nimport {useDropzone} from 'react-dropzone'\nimport { Box } from '@chakra-ui/react'\n\nconst DropzoneUpload = () => {\n\n    const onDrop = useCallback(acceptedFiles => {\n        // Do something with the files\n        console.log('got me some files', acceptedFiles)\n      }, [])\n\n      const {getRootProps, getInputProps, isDragActive} = useDropzone({onDrop})\n\n    return (\n        <Box {...getRootProps()}  w=\"100%\" p={4} color=\"white\" border=\"1px\" borderColor=\"gray.200\" borderStyle=\"dashed\" m=\"10px\" borderRadius=\"10px\" p=\"40px\">\n            <input {...getInputProps()} />\n                {\n                    isDragActive ?\n                    <p>Drop the files here ...</p> :\n                    <p>Drag 'n' drop some files here, or click to select files</p>\n                }\n        </Box>\n    )\n}\n\nexport default DropzoneUpload\n","/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/Albums/CreateNewAlbum.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/NotFound/NotFound.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/Albums/AllImages.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/Albums/AllAlbums.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/hooks/useAlbums.js",["113"],"import { useEffect, useState } from 'react'\nimport { db } from '../firebase/firebase'\nimport { useAuth } from '../contexts/ContextComp'\n\nconst useAlbums = () => {\n\tconst [albums, setAlbums] = useState([])\n\tconst [loading, setLoading] = useState(true)\n\n\tconst { currentUser } = useAuth()\n\n\tuseEffect(() => {\n\t\tconst unsubscribe = db.collection('albums')\n\t\t\t.where('owner', '==', currentUser.uid).onSnapshot(snapshot => {\n\t\t\t\tsetLoading(true)\n\t\t\t\tconst renderSnapAlbums = []\n\n\t\t\t\tsnapshot.forEach(doc => {\n\t\t\t\t\trenderSnapAlbums.push({\n\t\t\t\t\t\tid: doc.id,\n\t\t\t\t\t\t...doc.data()\n\t\t\t\t\t})\n\t\t\t\t})\n\n\t\t\tsetAlbums(renderSnapAlbums)\n\t\t\tsetLoading(false)\n\t\t})\n\n\t\treturn unsubscribe\n\t}, [])\n\n\treturn { albums, loading }\n}\n\nexport default useAlbums","/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/hooks/useAlbum.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/components/Albums/UploadImage.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/hooks/useDeleteImage.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/photo-review/src/hooks/useUploadImage.js",[],"/Applications/MAMP/htdocs/javascript-kurs-2/ph